import groovy.json.JsonOutput


def jsonSlurper = new JsonSlurper()
data = jsonSlurper.parse(new File(&quot;POST_Request.json&quot;))


sampler.setPostBodyRaw(true)

def newdata = JsonOutput.toJson(data)
sampler.addNonEncodedArgument(&quot;&quot;, newdata, &quot;&quot;)

log.info(&quot;JSON body: \n${newdata}&quot;)

