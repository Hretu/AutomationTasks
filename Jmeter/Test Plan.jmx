            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonOutput

// Read the JSON response from the previous sampler
def jsonResponse = prev.getResponseDataAsString()

// Parse the JSON content
def jsonSlurper = new JsonSlurper()
def jsonObject = jsonSlurper.parseText(jsonResponse)

// TODO add inversion
jsonObject.each {
	item -&gt;
	if(item.userId%2==1 &amp;&amp; item.id%2==0){
		item.completed = !item.completed
	}
}


//
// Convert the modified JSON object back to JSON string
def modifiedJson = JsonOutput.toJson(jsonObject)

// Update the response data with the modified JSON content
// prev.setResponseData(modifiedJson.getBytes(&quot;UTF-8&quot;))
File newFile = new File(&quot;POST_Request.json&quot;);
newFile.write(modifiedJson);
</stringProp>
